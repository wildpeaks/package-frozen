{
	"testExplorer.useNativeTesting": true,
	"mochaExplorer.require": "ts-node/register",
	"mochaExplorer.files": "test/*.test.ts",
	"eslint.validate": [
		"typescript"
	],
	"files.associations": {
		"tsconfig.json": "jsonc"
	},
	"files.exclude": {
		"LICENSE": true,
		".editorconfig": true,
		"package-lock.json": true,
		"node_modules": true
	}
}
